{
  "name": "nest-google-passport",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate-database": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -c migration"
  },
  "dependencies": {
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/cookie-session": "2.0.44",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-session": "2.0.0",
    "dotenv": "^16.0.2",
    "express-session": "^1.17.3",
    "js-yaml": "^4.1.0",
    "mysql2": "^2.3.3",
    "passport": "^0.5.3",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "typeorm": "0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "16.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.2",
    "prettier": "2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@repositories(.*)$": "<rootDir>/repositories$1",
      "^@storage(.*)$": "<rootDir>/storage$1",
      "^@services(.*)$": "<rootDir>/services$1",
      "^@guards(.*)$": "<rootDir>/guards$1",
      "^@interceptors(.*)$": "<rootDir>/interceptors$1",
      "^@decorators(.*)$": "<rootDir>/decorators$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@repositories": "dist/repositories",
    "@storage": "dist/storage",
    "@services": "dist/services",
    "@guards": "dist/guards",
    "@interceptors": "dist/interceptors",
    "@decorators": "dist/decorators"
  }
}
